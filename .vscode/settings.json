    //* 拡張機能 Auto Rename Tag キーワード一括リネーム設定
    "auto-rename-tag.activationOnLanguage": [
        "*"
    ],
    //* 拡張機能 Highlight Matching Tag HTMLタグの対応開始・終了タグのスタイル
    "highlight-matching-tag.styles": {
        // HTMLタグのハイライト色
        "opening": {
            "left": {
                "underline": "orange"
            },
            "right": {
                "surround": "#155FFA"
            },
            "name": {
                "highlight": "rgba(180, 20, 80, 0.3)"
            }
        },
        "closing": {
            "full": {
                "custom": {
                    "dark": {
                        "borderWidth": "0 0 1px 0",
                        "borderColor": "white",
                        "borderStyle": "solid",
                        "borderRadius": "4px",
                        "right": "10px"
                    },
                    "light": {
                        "borderWidth": "0 0 1px 0",
                        "borderColor": "red",
                        "borderStyle": "solid",
                        "borderRadius": "4px",
                        "right": "10px"
                    }
                }
            }
        }
    },
    //* ファイル処理
    "files.trimTrailingWhitespace": true,
    "files.eol": "\n",
    "files.insertFinalNewline": true, // ファイル最後に改行入れる
    "files.exclude": {
        ".vscode": false
    },
    "files.watcherExclude": {
        "**": false
    },
    "files.associations": {
        "**/templates/**/*.html": "django-html",
        "*.tpl": "django-html"
    },
    //* Gitでコミットするときにコメント入れるファイルを開かないようにする
    "git.autofetch": true,
    "git.useEditorAsCommitInput": false,
    // "gitlens.currentLine.dateFormat": "",// GitLens拡張機能用設定
    //* python用フォーマッター設定
    "pythonIndent.keepHangingBracketOnLine": false,
    "black-formatter.args": [
        "--skip-string-normalization"
    ],
    //* 拡張機能 Better-comments コメント色分けルール
    "better-comments.highlightPlainText": true,
    "better-comments.multilineComments": true,
    "better-comments.tags": [
        // pythonのコメントの色分けルール
        {
            "tag": "!", // ! 警告
            "color": "#FF2D00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "?", // ? 疑問
            "color": "#3498DB",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "//", // //取り消し線
            "color": "#474747",
            "strikethrough": true,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "todo", // TODO コメント
            "color": "#FF8C00",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": false,
            "italic": false
        },
        {
            "tag": "*", // * 強調
            "color": "#98C379",
            "strikethrough": false,
            "underline": false,
            "backgroundColor": "transparent",
            "bold": true,
            "italic": false
        }
    ],
