{
    //* ウィンドウのサイズ調整
    "window.zoomLevel": 0.8,
    "workbench.editor.tabSizing": "shrink", // ファイル名タブのサイズ
    "workbench.tree.indent": 10,
    "workbench.tree.renderIndentGuides": "always",
    "workbench.editor.wrapTabs": true,
    "workbench.iconTheme": "material-icon-theme", // アイコンテーマ
    "workbench.colorTheme": "Feather", // 画面のテーマ
    "workbench.colorCustomizations": {
        // カッコの色
        "editorBracketHighlight.foreground1": "#FF3335", // 赤
        "editorBracketHighlight.foreground2": "#FF8C00", // 橙
        "editorBracketHighlight.foreground3": "#E0BF01", // 黄(ゴールド)
        "editorBracketHighlight.foreground4": "#30C430", // 緑
        "editorBracketHighlight.foreground5": "#3F7FFF", // 青
        "editorBracketHighlight.foreground6": "#FF7FFF", // 紫
        // インデント
        "editorIndentGuide.background1": "#FF3335",
        "editorIndentGuide.background2": "#FF8C00",
        "editorIndentGuide.background3": "#E0BF01",
        "editorIndentGuide.background4": "#30C430",
        "editorIndentGuide.background5": "#3F7FFF",
        "editorIndentGuide.background6": "#FF7FFF",
        "editorIndentGuide.activeBackground1": "#fff",
        "editorIndentGuide.activeBackground2": "#fff",
        "editorIndentGuide.activeBackground3": "#fff",
        "editorIndentGuide.activeBackground4": "#fff",
        "editorIndentGuide.activeBackground5": "#fff",
        "editorIndentGuide.activeBackground6": "#fff",
        //* []にあるFeatherは選択しているテーマの名前にする
        "[Feather]": {
            //* FeatherはLightテーマ、Dark系テーマの時は行ハイライト色を変更した方がいい
            "editor.findMatchHighlightBackground": "#AA22FF22", // 検索キーワードの背景色（非選択）
            "editor.findMatchBackground": "#AA22FF11", // 検索キーワードの背景色（選択中）
            "editorSuggestWidget.selectedBackground": "#BAE8E9", // 補完入力で選択されている候補の背景色
            "editorStickyScroll.background": "#BAE9D1",
            "editorStickyScrollHover.background": "#A6C0B3",
            "editorStickyScroll.border": "#3A7A59",
            "editorStickyScroll.shadow": "#849c90",
            "editorLineNumber.foreground": "#405C79",
            "editorLineNumber.activeForeground": "#56BF8B",
            "editor.selectionBackground": "#98D98E", // 緑(選択背景)
            "editor.lineHighlightBackground": "#FAFA82", //行ハイライトの色
            // *"contrastActiveBorder": "#B0F010",  // アクティブタブの枠線
            "tab.activeBorder": "#FF6B00", // アクティブタブの境界線
            "tab.activeBackground": "#FFDD005E", //アクティブタブの背景色,
            "tab.activeForeground": "#33b101", // アクティブタブの文字色の変更
            "tab.inactiveForeground": "#192f60A0", // 非アクティブタブの文字色の変更
            "editorGroup.border": "#19405C79", // エディタを分割したときの境界線
            "sideBar.border": "#19405C79", // エクスプローラとエディタの境界線
            //* ターミナルの色変更 powershellは7をインストールするとこの設定が反映されます
            //* Base16 Terminal Colors for Visual Studio CodeにあるUnikitty Lightの文字色と背景色を変更
            "terminal.background": "#F7F9FB", // Darkがいい人は濃い色にする
            "terminal.foreground": "#405C79", // プロンプトの色
            "terminalCursor.background": "#405C79",
            "terminalCursor.foreground": "#405C79",
            "terminal.ansiBlack": "#FFFFFF",
            "terminal.ansiBlue": "#775DFF",
            "terminal.ansiBrightBlack": "#A7A5A8",
            "terminal.ansiBrightBlue": "#775DFF",
            "terminal.ansiBrightCyan": "#149BDA",
            "terminal.ansiBrightGreen": "#17AD98",
            "terminal.ansiBrightMagenta": "#AA17E6",
            "terminal.ansiBrightRed": "#D8137F",
            "terminal.ansiBrightWhite": "#322D34", // コマンド入力候補の色
            "terminal.ansiBrightYellow": "#DC8A0E", // コマンドの色
            "terminal.ansiCyan": "#149BDA",
            "terminal.ansiGreen": "#17AD98",
            "terminal.ansiMagenta": "#AA17E6",
            "terminal.ansiRed": "#D8137F",
            "terminal.ansiWhite": "#405C79",
            "terminal.ansiYellow": "#DC8A0E",
            //* ステータスバーの色変更
            "statusBar.background": "#405c79"
        }
    },
    //* プログラム内の括弧やダブルクォートの太字設定
    "editor.tokenColorCustomizations": {
        "[Feather]": {
            "numbers": "#0c9c84", // 数字の色
            "keywords": "#3193c6", // キーワードの色（importなど）
            "strings": "#8B56BF", // 文字列の色はデフォルト
            // "variables": "#FF00FF", //
            "types": "#155a5a",
            // "comments": "#FF0000", //   "コメントの色はデフォルト
            "textMateRules": [
                // // {
                // //   "scope": [
                // //     "constant.language",
                // //     "constant.character",
                // //     "constant.other"
                // //   ],
                // //   "settings": {
                // //     "foreground": "#ff2a04"
                // //   }
                // // },
                {
                    "scope": [
                        "string.quoted.docstring.multi"
                    ],
                    "settings": {
                        "foreground": "#919D9F",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": [
                        "support.type"
                    ],
                    "settings": {
                        "foreground": "#3193C6",
                        "fontStyle": ""
                    }
                },
                {
                    "scope": "keyword", // 記号などのキーワード
                    "settings": {
                        "fontStyle": "",
                        "foreground": "#7597b9"
                    }
                },
                {
                    "scope": [
                        "entity.name.function",
                        "support.function",
                        "meta.function-call.generic"
                    ],
                    "settings": {
                        "fontStyle": "italic",
                        "foreground": "#313131"
                    }
                },
                {
                    "scope": [
                        "punctuation.definition.dict.begin",
                        "punctuation.definition.dict.end",
                        "punctuation.definition.arguments.begin",
                        "punctuation.definition.arguments.end",
                        "punctuation.definition.list.begin",
                        "punctuation.definition.list.end",
                        "punctuation.definition.string.begin",
                        "punctuation.definition.string.end",
                        "punctuation.definition.parameters.begin",
                        "punctuation.definition.parameters.end",
                        "punctuation.parenthesis.begin",
                        "punctuation.parenthesis.end"
                    ],
                    "settings": {
                        "fontStyle": "bold"
                    }
                },
                {
                    //* python 関数の引数の色指定（元は黄色でLight背景では見えないため）
                    "scope": [
                        "variable.parameter.function-call"
                    ],
                    "settings": {
                        "foreground": "#f18136"
                    }
                },
                {
                    "scope": "support.type.property-name",
                    "settings": {
                        "foreground": "#3193C6",
                        "fontStyle": "bold"
                    }
                },
                {
                    "scope": [
                        "variable.parameter.function",
                        "meta.function-call.arguments"
                    ],
                    "settings": {
                        "foreground": "#395063",
                        "fontStyle": ""
                    }
                }
                // {
                //   // 警告とエラーの波線の色
                //   "scope": ["editorWarning.foreground", "editorError.foreground"],
                //   "settings": {
                //     "foreground": "#FF0000",
                //     "fontStyle": "bold"
                //   }
                // }
            ]
        }
    },
    "editor.hover.enabled": true, // ホバー表示
    "editor.guides.bracketPairs": true,
    "editor.stickyScroll.enabled": false, // カーソル位置にあるコードがどこのメソッド（タグ）内か表示
    "editor.minimap.enabled": false, // ミニマップ表示
    //* マウスホイールによるフォントサイズ変更
    "editor.mouseWheelZoom": true,
    "editor.fontSize": 16,
    "editor.wordWrap": "on",
    "editor.tabSize": 4,
    "editor.insertSpaces": true, // タブとスペースの変換しない
    // 開いたファイルのインデントとタブ/スペース変換はこのvscodeの設定に従うための設定（false）
    "editor.detectIndentation": false,
    "editor.fontFamily": "'Myrica MM','BIZ UDゴシック', Consolas, 'Courier New', monospace",
    "editor.formatOnPaste": true,
    "editor.formatOnType": true,
    // "editor.defaultFormatter": "esbenp.prettier-vscode", // 拡張機能Prettier - Code formatter
    "editor.formatOnSave": true,
    "[django-html]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "junstyle.vscode-django-support"
    },
    "[html]": {
        "editor.defaultFormatter": "vscode.html-language-features"
    },
    // "[css]": {
    //   "editor.defaultFormatter": "esbenp.prettier-vscode"
    // },
